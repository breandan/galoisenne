{
  "properties": {
    "kaliningraphVersion": "0.0.4",
    "graphvizVersion": "0.16.0",
    "jgraphtVersion": "1.4.0",
    "ejmlVersion": "0.39",
    "mathVersion": "3.6.1",
    "tinkerpopVersion": "3.4.6"
  },
  "link": "https://github.com/breandan/kaliningraph",
  "repositories": [
    "https://jitpack.io"
  ],
  "dependencies": [
    "com.github.breandan:kaliningraph:$kaliningraphVersion",
    "guru.nidi:graphviz-kotlin:$graphvizVersion",
    "org.ejml:ejml-kotlin:$ejmlVersion",
    "org.ejml:ejml-all:$ejmlVersion",
    "org.apache.commons:commons-math3:$mathVersion",
    "org.jgrapht:jgrapht-core:$jgraphtVersion",
    "org.jgrapht:jgrapht-ext:$jgraphtVersion",
    "org.apache.tinkerpop:gremlin-core:$tinkerpopVersion",
    "org.apache.tinkerpop:tinkergraph-gremlin:$tinkerpopVersion"
  ],
  "imports": [
    "edu.mcgill.kaliningraph.*",
    "edu.mcgill.kaliningraph.circuits.*"
  ],
  "renderers": {
    "edu.mcgill.kaliningraph.Graph": "HTML(($it as edu.mcgill.kaliningraph.Graph).html())",
    "edu.mcgill.kaliningraph.circuits.Gate": "HTML(Graph($it as edu.mcgill.kaliningraph.circuits.Gate).html())",
    "edu.mcgill.kaliningraph.circuits.NFunction": "HTML(Graph($it as edu.mcgill.kaliningraph.circuits.Gate).html())",
    "org.ejml.data.DMatrixSparseCSC": "HTML(\"<img src=\\\"${graph.A.adjToMat()}\\\"/>\")"
  }
}